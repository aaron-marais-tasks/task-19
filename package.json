{
  "name": "task-19",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "React": "npm run start --prefix ReactApp",
    "Express": "npm run start --prefix ExpressApp",
    "start": "run-p -cn React Express",
    "dev:React": "npm run React",
    "dev:Express": "cd ExpressApp && nodemon --exec \"npm start\" --ignore \"favorites.json\"",
    "dev": "run-p -cn dev:*",
    "install:React": "cd ReactApp && npm install",
    "install:Express": "cd ExpressApp && npm install",
    "prepare": "run-p install:*"
  }
}
